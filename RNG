import java.util.Random;

public class RNG{
    Random generator;
    public  RNG(){
        generator = new Random();
    }
    public void tafelrng(){
            // 0 <= zuffi < 10 
            int zuffi = generator.nextInt(6);
            zuffi = zuffi +1;
            System.out.println(zuffi);
    }
}
